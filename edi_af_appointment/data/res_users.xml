<odoo>
    <data>    
        <record id="partner_sunea" model="res.partner">
            <field name="name">sunea</field>
            <field name="company_id" ref="base.main_company"/>
            <field name="customer" eval="False"/>
            <field name="company_name">YourCompany</field>
            <field name="street">Elverksgatan 4</field>
            <field name="city">Solna</field>
            <!-- <field name="state_id"  model="res.country.state" search="[('code','ilike','OR')]"/> -->
            <field name="zip">97401</field>
            <field name="country_id" ref="base.se"/>
            <field name="tz">Europe/Brussels</field>
