<openerp>
    <data noupdate="1">

        <record id="schedule" model="edi.route">
            <field name="name">Schedule route</field>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="active" eval="True"/>
            <field name="protocol">ipf</field>
            <field name="route_type">edi_af_schedules</field>
        </record>

        <record id="ace_wi" model="edi.route">
            <field name="name">ACE WI route</field>
            <field name="partner_id" ref="base.main_partner"/>
            <field name="active" eval="True"/>
            <field name="protocol">ipf</field>
            <field name="route_type">edi_af_ace_wi</field>
        </record>

        <record id="appointment_schedules" model="edi.message.type">
            <field name="name">AF appointment: Get schedules</field>
            <field name="target">appointments/v2/resource-planning/competencies/schedules</field>
            <field name="type_mapping">{{url}}:{{port}}/{path}?client_id={{client}}&amp;client_secret={{secret}}&amp;from_date={from_date}&amp;to_date={to_date}&amp;competence_id={comp}</field>
        </record>

        <record id="appointment_ace_wi" model="edi.message.type">
            <field name="name">AF appointment: Post ACE WI</field>
            <field name="target">appointments/v2/phone-appointments/queues/{queueId}/workitems</field>
            <field name="type_mapping">{{url}}:{{port}}/{path}?client_id={{client}}&amp;client_secret={{secret}}</field>
        </record>

    </data>
</openerp>