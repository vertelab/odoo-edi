<odoo>
    <data noupdate="1">
        <record id="ais_bos_route" model="edi.route">
            <field name="name">AF Asok: Get AIS BOS route</field>
            <field name="partner_id" ref="base.main_partner"/> 
            <field name="active" eval="True"/> 
            <field name="protocol">ipf</field> 
            <field name="route_type">edi_af_as_ais_bos</field>
        </record>

        <record id="krom_postcode" model="edi.message.type">
            <field name="name">AF asok: Get BOS KROM Postcode</field>
            <field name="target">ais-bos-regelverk/v2/krom/kromtyp-for-postnummer</field>
            <field name="type_mapping">{{url}}:{{port}}/{path}?client_id={{client}}&amp;client_secret={{secret}}&amp;postnummer={postnummer}</field>
        </record>

    </data>
</odoo>
